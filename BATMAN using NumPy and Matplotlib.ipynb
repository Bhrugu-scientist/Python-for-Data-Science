Y = np.arange(-4,4,0.005)
X = np.zeros((0))
for y in Y:
    X = np.append(X,abs(y/2)-0.09137*y**2+math.sqrt(1-(abs(abs(y)-2)-1)**2)-3)
    
    
Y1 = np.append(np.arange(-7,-3,0.1),np.arange(3,7,0.1))
X1 = np.zeros((0))
for y in Y1:
    X1 = np.append(X1,3*math.sqrt(-(y/7)**2+1))
    

X = np.append(X,X1)
Y = np.append(Y,Y1)

